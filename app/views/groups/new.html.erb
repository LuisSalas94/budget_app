<%= form_with url: "/groups", scope: :group do |f| %>
<nav>
  <div >
    <ul >
        <li >
        <%= link_to '<', '/' %>
        </li>
        </ul>
    <a  href="#"><%= @title.upcase || BudgetApp %></a>
  <%= f.submit 'Create' %>
  </div>
</nav>
  <%= f.text_field :name, placeholder: 'Category Name'%>
  <%= f.text_field :icon, placeholder: 'Icon Name'%>
  <%= f.hidden_field :user_id, value: current_user.id %>
<% end %>